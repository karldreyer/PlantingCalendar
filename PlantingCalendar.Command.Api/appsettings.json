{
  "SeriLog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "CouchDb": {
    "Server": "localhost",
    "DatabaseName": "planting-calendar",
    "Username": "admin",
    "Password":  "admin"
  },
  "AllowedHosts": "*"
}
